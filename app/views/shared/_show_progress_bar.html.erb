<!-- 25ì  -->
<% if var <= 25 %>
	<div class="progress" >

		<div class="progress-bar progres-bar-success" role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="25" style="width: 25%;">
		    <span class="sr-only">0% Complete</span>
		</div>

		<div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="25" style="width: 25%;">
		    <span class="sr-only">0% Complete</span>
		</div>

		<div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="0" aria-valuemin="0"  aria-valuemax="25" style="width: 25%;">
		    <span class="sr-only">0% Complete</span>
		</div>

		<div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="0" aria-valuemin="0"  aria-valuemax="25" style="width: 25%;">
		    <span class="sr-only">0% Complete</span>
		</div>
	</div>  



