<li class = "dropdown">
  <a href= "#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">시간표짜기 <span class="caret"></span>
  </a>    

  <ul class="dropdown-menu">
    <!-- timetables.each문 통해 timetable개수만큼 li문 만들기 -->

    <% session[:timetables].each do |timetable| %>

    	<li>
    		<a href="<%= timetable_path(timetable) %>"> 
    			<%= timetable.name %> 
    		</a>
    	</li>

    <% end %>

    <li role="separator" class="divider"></li>
    <li>		
    	<%= link_to "시간표 추가하기", new_timetable_path, method: :post %>
	</li>
  </ul>
</li> 